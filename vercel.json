{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "functions": {
    "src/app/api/chat/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/search/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/sources/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/scrape/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/screenshot/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/api/browse/route.ts": {
      "maxDuration": 45
    },
    "src/app/api/images/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/url/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/proxy/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/video-extract/route.ts": {
      "maxDuration": 45
    },
    "src/app/api/video-proxy/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Range"
        },
        {
          "key": "Access-Control-Expose-Headers",
          "value": "Content-Range, Content-Length, Accept-Ranges"
        }
      ]
    }
  ]
}
