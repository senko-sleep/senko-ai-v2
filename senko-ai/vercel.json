{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "functions": {
    "src/app/api/chat/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/search/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/sources/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/scrape/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/screenshot/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/images/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/url/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/browse/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/proxy/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/video-extract/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/video-proxy/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/list-links/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/navigate/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
